#include <sys/types.h>
#include <unistd.h>
#include <stdio.h>

int main(int argc, char *argv[])
{
  int pid;
	int status=0;
	pid=fork();
	if (pid>0)
		{ printf ("Parent: child has pid=%d",pid);
			pid=wait(&status);

		}else if (pid=0)
		{ printf ("Child here");
			exit(status);

		}else
		{perror("fork problem");
			exit(-1);
			}
	return 0;
}



